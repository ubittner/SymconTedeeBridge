{
    "elements": [
        {
            "type": "Image",
            "image": "data:image/png;base64, "
        },
        {
            "type": "Label",
            "caption": "Bridge (Local API)"
        },
        {
            "type": "Label",
            "caption": "Version:"
        },
        {
            "type": "Label",
            "caption": " "
        },
        {
            "type": "CheckBox",
            "name": "Active",
            "caption": "Active"
        },
        {
            "type": "Label",
            "caption": " "
        },
        {
            "type": "ValidationTextBox",
            "name": "BridgeIP",
            "caption": "IP Address"
        },
        {
            "type": "NumberSpinner",
            "name": "BridgePort",
            "caption": "Port"
        },
        {
            "type": "NumberSpinner",
            "name": "Timeout",
            "caption": "Network Timeout",
            "suffix": "Milliseconds"
        },
        {
            "type": "NumberSpinner",
            "name": "ExecutionTimeout",
            "caption": "Execution Timeout",
            "suffix": "Seconds"
        },
        {
            "type": "Label",
            "caption": " "
        },
        {
            "type": "CheckBox",
            "name": "UseEncryption",
            "caption": "Encrypt token",
            "width": "300px"
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "PasswordTextBox",
                    "name": "Token",
                    "caption": "Token"
                },
                {
                    "type": "Button",
                    "caption": "Tedee documentation",
                    "onClick": "echo 'https://docs.tedee.com/bridge-api#tag/Getting-started/Enabling-Bridge-API';"
                }
            ]
        },
        {
            "type": "Label",
            "caption": " "
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "CheckBox",
                    "name": "UseCallback",
                    "caption": "Use automatic update",
                    "width": "300px"
                },
                {
                    "type": "PopupButton",
                    "caption": "Info",
                    "popup": {
                        "caption": "Use automatic update",
                        "items": [
                            {
                                "type": "Label",
                                "caption": "Activate this function to update the status automatically via Callback."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "type": "ValidationTextBox",
            "name": "SocketIP",
            "caption": "Host IP-Address (IP-Symcon)"
        },
        {
            "type": "NumberSpinner",
            "name": "SocketPort",
            "caption": "Host Port (IP-Symcon)"
        }
    ],
    "actions": [
        {
            "type": "ExpansionPanel",
            "caption": "Developer area",
            "items": [
                {
                    "type": "Button",
                    "caption": "Bridge Info",
                    "onClick": "$info = TB_GetBridgeInfo($id); print_r(json_decode($info, true));"
                },
                {
                    "type": "Button",
                    "caption": "Paired Locks",
                    "onClick": "$locks = TB_GetPairedLocks($id); print_r(json_decode($locks, true));"
                },
                {
                    "type": "Button",
                    "caption": "Show Callbacks",
                    "onClick": "$data = TB_GetCallbacks($id); print_r(json_decode($data, true));"
                },
                {
                    "type": "PopupButton",
                    "caption": "Add Callback",
                    "popup": {
                        "caption": "Do you really want to add a callback?",
                        "items": [
                            {
                                "type": "Button",
                                "caption": "Add",
                                "onClick": "$data = TB_AddCallback($id); print_r(json_decode($data, true));"
                            }
                        ]
                    }
                },
                {
                    "type": "PopupButton",
                    "caption": "Delete Callback",
                    "popup": {
                        "caption": "Do you really want to delete a callback?",
                        "items": [
                            {
                                "type": "NumberSpinner",
                                "name": "CallbackID",
                                "caption": "Callback ID",
                                "value": 1
                            },
                            {
                                "type": "Button",
                                "caption": "Delete",
                                "onClick": "$data = TB_DeleteCallback($id, $CallbackID); print_r(json_decode($data, true));"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "status": [
        {
            "code": 101,
            "icon": "active",
            "caption": "Tedee Bridge (Local API) will be created"
        },
        {
            "code": 102,
            "icon": "active",
            "caption": "Tedee Bridge (Local API) is active"
        },
        {
            "code": 103,
            "icon": "active",
            "caption": "Tedee Bridge (Local API) will be deleted"
        },
        {
            "code": 104,
            "icon": "inactive",
            "caption": "Tedee Bridge (Local API) is inactive"
        },
        {
            "code": 200,
            "icon": "inactive",
            "caption": "An error has occurred, for further information see messages, log file or debug!"
        },
        {
            "code": 201,
            "icon": "inactive",
            "caption": "Please check your configuration!"
        }
    ]
}